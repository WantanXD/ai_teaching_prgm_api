// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Authenticate {
  id    Int     @id @default(autoincrement())
  name  String
  email String  @unique
  pass  String
  salt  Int
}

model QandAData {
  id            Int     @id @default(autoincrement())
  userId        Int
  lang          String
  tof           Boolean
  question      String
  answer        String
  model_answer  String
  comment       String
}